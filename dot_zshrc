autoload -Uz compinit
compinit

export PATH=${PATH}:${HOME}/.local/bin:${HOME}/.cargo/bin
eval "$(mise activate zsh)"
eval "$(starship init zsh)"
eval "$(sheldon source)"


ZSHHOME="${HOME}/.zsh.d"
if [ -d $ZSHHOME -a -r $ZSHHOME -a \
    -x $ZSHHOME ]; then
    for i in $ZSHHOME/*; do
        [[ ${i##*/} = *.zsh ]] &&
            [ \( -f $i -o -h $i \) -a -r $i ] && . $i
    done
fi
